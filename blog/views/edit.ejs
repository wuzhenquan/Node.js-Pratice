<%- include header %>
    <form method="post">
        title:
        <br/>
        <input type="text" name="title" value="<%= post.title%>" disabled/>
        <br/> 
        tags:
        <br/>
        <input type="text" name="tag1" value="<%= post.tags[0]%>" disabled/>
        <input type="text" name="tag2" value="<%= post.tags[1]%>" disabled/>
        <input type="text" name="tag3" value="<%= post.tags[2]%>" disabled/>
        <br/> 
        post:
        <br/>
        <textarea name="post" rows="20" cols="100"><%= post.post%></textarea>
        <br/>
        <input type="submit" value="post">
    </form>
<%- include footer %>